        "@mdx-js/react": "^3.1.1",
        "@mdxeditor/editor": "^3.52.4",
        "@shikijs/rehype": "^3.7.0",
        "@supabase/supabase-js": "^2.93.3",
        "astro": "^5.17.1",
        "cm6-theme-basic-dark": "^0.2.0",
        "cm6-theme-basic-light": "^0.2.0",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",

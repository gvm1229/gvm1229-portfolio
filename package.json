{
    "name": "folium-online",
    "type": "module",
    "version": "0.0.1",
    "engines": {
        "node": ">=24.13.0"
    },
    "scripts": {
        "dev": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "format": "prettier --write .",
